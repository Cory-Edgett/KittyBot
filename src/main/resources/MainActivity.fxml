<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Text layoutX="14.0" layoutY="19.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Guilds" />
            <Text layoutX="14.0" layoutY="144.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Channels" />
            <ListView fx:id="guilds" layoutX="14.0" layoutY="22.0" onMouseClicked="#selectGuild" prefHeight="92.0" prefWidth="282.0" />
            <ListView fx:id="channels" layoutX="14.0" layoutY="152.0" onMouseClicked="#selectChannel" prefHeight="92.0" prefWidth="282.0" />
            <TextArea fx:id="messageBox" layoutX="9.0" layoutY="275.0" prefHeight="144.0" prefWidth="625.0" />
            <Button fx:id="send" layoutX="581.0" layoutY="376.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
            <Text layoutX="329.0" layoutY="19.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Chat" />
            <ListView fx:id="chat" layoutX="329.0" layoutY="22.0" prefHeight="222.0" prefWidth="282.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
